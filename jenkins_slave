
pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Tuchsanai', description: 'Enter your name')
        string(name: 'Ages', defaultValue: '21', description: 'Enter your age')
    }
    stages {
        stage('Example') {
            steps {
                script{
                    def file = readFile(file: 'data.txt')
                    echo file
                    sh 'python myscript.py ${NAME} --age ${Ages}'
                }
            }
        }
    }
}
